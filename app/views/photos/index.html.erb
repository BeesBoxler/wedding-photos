<h1>Listing photos</h1>

<div id="container">
  <% @photos.each do |photo| %>
    <div class="photo">
      <%= image_tag photo.image.url(:small) %>
      <p>Uploaded by <%= photo.user.name %></p>
      <%= link_to 'View', photo %>
      <%= link_to 'Delete', photo, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>
</div>